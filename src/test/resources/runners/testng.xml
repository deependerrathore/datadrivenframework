<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Data Driven Framework">
	<listeners>
		<listener class-name="org.uncommons.reportng.HTMLReporter" />
		<listener
			class-name="org.uncommons.reportng.JUnitXMLReporter" />
		<listener class-name="com.deepak.listeners.CustomListeners" />
	</listeners>
	<test thread-count="5" name="Login Test">
		<classes>
			<class name="com.deepak.testcases.BankManagerLoginTest" />
		</classes>
	</test>
	<test name="Add customer test">
		<classes>
			<class name="com.deepak.testcases.AddCustomerTest" />
		</classes>
	</test>
	<test name="Open Account test">
		<classes>
			<class name="com.deepak.testcases.OpenAccountTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
